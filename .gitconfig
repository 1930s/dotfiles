[core]
	editor = nvim
[user]
	name = Andreas Thoelke
	email = andreas.thoelke@gmail.com
[pager]
	status = true
[diff]
	tool = vimdiff
[alias]
	co = checkout
  ci = commit
  st = status
  br = branch
  unstage = reset HEAD --
  ap = add --patch
  tl = tag -l -n5
  tagls = tag -l -n5
  last = log -1 HEAD
  hist = log --pretty=format:\" %s%d | %ad | %h \" --graph --date=short
  visual = !gitk
data=short
  type = cat-file -t
  dump = cat-file -p





